{
  "$schema": "https://biomejs.dev/schemas/1.8.0/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "a11y": {
        "noSvgWithoutTitle": "off"
      }
    }
  },
  "css": {
    "parser": {
      "cssModules": true
    }
  },
  "overrides": [
    {
      "ignore": ["**/*.css"]
    }
  ]
}
